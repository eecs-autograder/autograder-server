components:
  schemas:
    User:
      type: object
      properties:
        pk: &id010
          type: integer
          format: id
          read_only: true
        username:
          read_only: false
          description: Required. 150 characters or fewer. Letters, digits and @/./+/-/_
            only.
          nullable: false
          type: string
        first_name:
          read_only: false
          description: ''
          nullable: false
          type: string
        last_name:
          read_only: false
          description: ''
          nullable: false
          type: string
        email:
          read_only: false
          description: ''
          nullable: false
          type: string
          format: email
        is_superuser:
          read_only: false
          description: Designates that this user has all permissions without explicitly
            assigning them.
          nullable: false
          type: boolean
    Course:
      type: object
      properties:
        pk: *id010
        name:
          read_only: false
          description: The name of this course. Must be unique, non-empty and non-null.
          nullable: false
          type: string
        semester:
          read_only: false
          description: ''
          nullable: true
          oneOf:
          - $ref: '#/components/schemas/Semester'
        year:
          read_only: false
          description: ''
          nullable: true
          type: integer
        subtitle:
          read_only: false
          description: An optional descriptive name for the course.
          nullable: false
          type: string
        num_late_days:
          read_only: false
          description: The number of late days to give to users submitting to this
            course's projects.
          nullable: false
          type: integer
        allowed_guest_domain:
          read_only: false
          description: "When non-empty, indicates that guest users' usernames\n  \
            \                   must end with this string for them to be allowed access\n\
            \                     to projects that allow guests.\n               \
            \      When empty, indicates that all guests can access projects\n   \
            \                  in this course where guests are allowed access."
          nullable: false
          type: string
        last_modified:
          read_only: true
          description: ''
          nullable: false
          type: string
          format: date-time
    Semester:
      type: string
      enum:
      - Fall
      - Winter
      - Spring
      - Summer
    Project:
      type: object
      properties:
        pk: *id010
        name:
          read_only: false
          description: "The name used to identify this project.\n            Must\
            \ be non-empty and non-null.\n            Must be unique among Projects\
            \ associated with\n            a given course.\n            This field\
            \ is REQUIRED."
          nullable: false
          type: string
        last_modified:
          read_only: true
          description: ''
          nullable: false
          type: string
          format: date-time
        course: *id010
        visible_to_students:
          read_only: false
          description: "Whether information about this Project can\n            be\
            \ viewed by students."
          nullable: false
          type: boolean
        closing_time:
          read_only: false
          description: "The date and time that this project should stop\n        \
            \    accepting submissions.\n            A value of None indicates that\
            \ this project should\n            stay open.\n            In the API,\
            \ this field is hidden from non-admins."
          nullable: true
          type: string
          format: date-time
        soft_closing_time:
          read_only: false
          description: "The date and time that should be displayed as the\n      \
            \      due date for this project. Unlike closing_time,\n            soft_closing_time\
            \ does not affect whether submissions are\n            actually accepted.\n\
            \            If not None and closing_time is not None, this value must\
            \ be\n            less than (before) closing_time."
          nullable: true
          type: string
          format: date-time
        disallow_student_submissions:
          read_only: false
          description: "A hard override that indicates that students should\n    \
            \        be prevented from submitting even if visible_to_students is\n\
            \            True and it is before closing_time."
          nullable: false
          type: boolean
        disallow_group_registration:
          read_only: false
          description: "A hard override that indicates that students should\n    \
            \        not be able to send, accept, or reject group\n            invitations."
          nullable: false
          type: boolean
        guests_can_submit:
          read_only: false
          description: "By default, only admins, staff, and students\n           \
            \ for a given Course can view and submit to its Projects.\n          \
            \  When True, submissions will be accepted from guests\n            with\
            \ the following caveats:\n                - Guests must be given a direct\
            \ link to the project.\n                - When group work is allowed,\
            \ guests can\n                only be in groups with other guests."
          nullable: false
          type: boolean
        min_group_size:
          read_only: false
          description: "The minimum number of students that can work in a\n      \
            \      group on this project.\n            Must be >= 1.\n           \
            \ Must be <= max_group_size."
          nullable: false
          type: integer
        max_group_size:
          read_only: false
          description: "The maximum number of students that can work in a\n      \
            \      group on this project.\n            Must be >= 1.\n           \
            \ Must be >= min_group_size."
          nullable: false
          type: integer
        submission_limit_per_day:
          read_only: false
          description: "The number of submissions each group is allowed per\n    \
            \        day before either reducing feedback or preventing further\n \
            \           submissions. A value of None indicates no limit."
          nullable: true
          type: integer
        allow_submissions_past_limit:
          read_only: false
          description: "Whether to allow additional submissions after a\n        \
            \    group has submitted submission_limit_per_day times."
          nullable: false
          type: boolean
        groups_combine_daily_submissions:
          read_only: false
          description: "If True, group members can \"pool\" their daily submissions.\n\
            \            For example, if submission_limit_per_day is 3,\n        \
            \    a group with 2 members would get 6 submissions per day."
          nullable: false
          type: boolean
        submission_limit_reset_time:
          read_only: false
          description: "The time that marks the beginning and end of the 24\n    \
            \        hour period during which submissions should be counted\n    \
            \        towards the daily limit. Defaults to 00:00:00."
          nullable: false
          type: string
          format: time
        submission_limit_reset_timezone:
          read_only: false
          description: "The timezone to use when computing how many\n            submissions\
            \ a group has made in a 24 hour period."
          nullable: false
          type: string
          format: timezone
        num_bonus_submissions:
          read_only: false
          description: ''
          nullable: false
          type: integer
        total_submission_limit:
          read_only: false
          description: "The maximum number of times a Group can submit to\n      \
            \      this Project EVER."
          nullable: true
          type: integer
        allow_late_days:
          read_only: false
          description: "Whether to allow the use of late days for submitting\n   \
            \         past the deadline."
          nullable: false
          type: boolean
        ultimate_submission_policy:
          read_only: false
          description: "The \"ultimate\" submission for a group is the one\n     \
            \       that will be used for final grading. This field specifies\n  \
            \          how the ultimate submission should be determined."
          nullable: false
          oneOf:
          - $ref: '#/components/schemas/UltimateSubmissionPolicy'
        hide_ultimate_submission_fdbk:
          read_only: false
          description: "A hard override that indicates that ultimate\n           \
            \ submission feedback should not be shown, even if the\n            appropriate\
            \ criteria are met."
          nullable: false
          type: boolean
        instructor_files:
          read_only: true
          description: ''
          nullable: false
          type: array
          items:
            $ref: '#/components/schemas/InstructorFile'
        expected_student_files:
          read_only: true
          description: ''
          nullable: false
          type: array
          items:
            $ref: '#/components/schemas/ExpectedStudentFile'
        has_handgrading_rubric:
          readOnly: true
          description: Whether this project has a handgrading rubric.
          type: boolean
    UltimateSubmissionPolicy:
      type: string
      enum:
      - most_recent
      - best_basic_score
      - best
    ExpectedStudentFile:
      type: object
      properties:
        pk: *id010
        project: *id010
        pattern:
          read_only: false
          description: "A shell-style file pattern suitable for\n            use with\
            \ Python's fnmatch.fnmatch()\n            function (https://docs.python.org/3.5/library/fnmatch.html)\n\
            \            This string must be a legal UNIX filename and may not be\n\
            \            '..' or '.'.\n            NOTE: Patterns for a given project\
            \ must not overlap,\n                otherwise the behavior is undefined."
          nullable: false
          type: string
        min_num_matches:
          read_only: false
          description: "The minimum number of submitted student files that\n     \
            \       should match the pattern. Must be non-negative."
          nullable: false
          type: integer
        max_num_matches:
          read_only: false
          description: "The maximum number of submitted student files that\n     \
            \       can match the pattern. Must be >= min_num_matches"
          nullable: false
          type: integer
        last_modified:
          read_only: true
          description: ''
          nullable: false
          type: string
          format: date-time
    InstructorFile:
      type: object
      properties:
        pk: *id010
        project: *id010
        name:
          read_only: true
          description: ''
          nullable: false
          type: string
        last_modified:
          read_only: true
          description: ''
          nullable: false
          type: string
          format: date-time
        size:
          readOnly: true
          description: ''
          type: integer
    DownloadTask:
      type: object
      properties:
        pk: *id010
        project: *id010
        download_type:
          read_only: true
          description: ''
          nullable: false
          oneOf:
          - $ref: '#/components/schemas/DownloadType'
        result_filename:
          read_only: false
          description: ''
          nullable: false
          type: string
        progress:
          read_only: false
          description: A percentage indicating how close the task is to completion.
          nullable: false
          type: integer
        error_msg:
          read_only: false
          description: ''
          nullable: false
          type: string
        created_at:
          read_only: true
          description: ''
          nullable: false
          type: string
          format: date-time
    DownloadType:
      type: string
      enum:
      - all_scores
      - final_graded_submission_scores
      - all_submission_files
      - final_graded_submission_files
    Group:
      type: object
      properties:
        pk: *id010
        project: *id010
        extended_due_date:
          read_only: false
          description: "When this field is set, it indicates that members\n      \
            \      of this submission group can submit until this specified\n    \
            \        date, overriding the project closing time.\n            Default\
            \ value: None"
          nullable: true
          type: string
          format: date-time
        member_names:
          readOnly: true
          description: A list of usernames of the group members, sorted alphabetically.
          type: array
          items: &id011
            type: string
        bonus_submissions_remaining:
          read_only: false
          description: "The number of bonus submissions this group has left.\n   \
            \         This field is automatically initialized to self.project.num_bonus_submissions"
          nullable: false
          type: integer
        late_days_used:
          read_only: true
          description: "Keeps track of how many late days each user in this\n    \
            \        group has used.\n            Data format: {\n               \
            \ \"\\<username\\>\": \\<num late days used\\>,\n                ...\n\
            \            }\n            NOTE: This field is updated only when a group\
            \ member uses a\n            late day. If a user is moved to another group\
            \ or this group\n            is merged with another one, this field will\
            \ NOT be updated."
          nullable: false
          type: object
        num_submissions:
          readOnly: true
          description: ''
          type: integer
        num_submits_towards_limit:
          readOnly: true
          description: "The number of submissions this group has made in the current\
            \ 24\n        hour period that are counted towards the daily submission\
            \ limit."
          type: integer
        created_at:
          read_only: true
          description: ''
          nullable: false
          type: string
          format: date-time
        last_modified:
          read_only: true
          description: ''
          nullable: false
          type: string
          format: date-time
    GroupInvitation:
      type: object
      properties:
        pk: *id010
        invitation_creator: *id010
        project: *id010
        invited_usernames:
          readOnly: true
          description: The usernames of the Users that will receive this invitation.
          type: array
          items: *id011
        invitees_who_accepted:
          readOnly: true
          description: "A list of usernames indicating which invitees have accepted\n\
            \        this invitation.\n        This field is READ ONLY."
          type: array
          items: *id011
    Submission:
      type: object
      properties:
        pk: *id010
        group: *id010
        timestamp:
          read_only: true
          description: ''
          nullable: false
          type: string
          format: date-time
        submitter:
          read_only: true
          description: The name of the user who made this submission
          nullable: false
          type: string
        submitted_filenames:
          read_only: true
          description: "The names of files that were submitted,\n                \
            \     excluding those that were discarded."
          nullable: false
          type: array
          items: &id012
            type: string
        discarded_files:
          read_only: true
          description: The names of files that were discarded when this Submission
            was created.
          nullable: false
          type: array
          items: *id012
        missing_files:
          read_only: true
          description: "Stores missing filenames and the additional number\n     \
            \       of files needed to satisfy a file pattern requirement.\n     \
            \       Stored as key-value pairs of the form:\n            {pattern:\
            \ num_additional_needed}"
          nullable: false
          type: object
        status:
          read_only: true
          description: "The grading status of this submission see\n            Submission.GradingStatus\
            \ for details on allowed values."
          nullable: false
          type: string
        count_towards_daily_limit:
          read_only: false
          description: "Indicates whether this submission should count\n         \
            \   towards the daily submission limit."
          nullable: false
          type: boolean
        is_past_daily_limit:
          read_only: true
          description: Whether this submission is past the daily submission limit.
          nullable: false
          type: boolean
        is_bonus_submission:
          read_only: true
          description: "When True, indicates that the group that made this\n     \
            \       submission should be able to request normal feedback for\n   \
            \         this submission's results.\n            Note: If this field\
            \ is True, is_past_daily_limit should be\n            False."
          nullable: false
          type: boolean
        count_towards_total_limit:
          read_only: false
          description: Whether this submission should count towards the total submission
            limit.
          nullable: false
          type: boolean
        does_not_count_for:
          read_only: true
          description: "A list of users for whom this submission will NOT\n      \
            \      count as their final graded submission. Users are added to\n  \
            \          this list if they are out of late days and another group\n\
            \            member (who still has late days remaining) uses their own\n\
            \            late day to submit."
