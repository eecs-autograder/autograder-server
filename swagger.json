          read_only: true
          description: ''
          nullable: false
          type: string
          format: date-time
    Command:
      type: object
      properties:
        cmd:
          description: "A string containing the command to be run.\n             \
            \     Note: This string will be inserted into ['bash', '-c', <cmd>]\n\
            \                     in order to be executed."
          type: string
        name:
          description: An optional name for this command to be displayed to users.
          type: string
        time_limit:
          description: "The time limit in seconds to be placed on the command.\n \
            \           Must be > 0\n            Must be <= 90"
          type: integer
        stack_size_limit:
          description: "The maximum stack size in bytes.\n            Must be > 0\n\
            \            Must be <= 100000000\n            NOTE: Setting this value\
            \ too low may cause the command to crash prematurely."
          type: integer
        virtual_memory_limit:
          description: "The maximum amount of virtual memory (in bytes) allowed.\n\
            \            Must be > 0\n            Must be <= 4000000000\n        \
            \    NOTE: Setting this value too low may cause the command to crash prematurely."
          type: integer
        process_spawn_limit:
          description: "The maximum number of processes the command is allowed to\
            \ spawn.\n            Must be >= 0\n            Must be <= 150\n     \
            \       NOTE: This limit applies cumulatively to the processes\n     \
            \             spawned by the main program being run. i.e. If a\n     \
            \             spawned process spawns it's own child process, both\n  \
            \                of those processes will count towards the main\n    \
            \              program's process limit."
          type: integer
