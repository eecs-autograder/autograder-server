    RerunSubmissionsTask:
      type: object
      properties:
        pk: *id010
        progress:
          readOnly: true
          description: ''
          type: integer
        is_cancelled:
          read_only: true
          description: Indicates whether the task has been cancelled by the user.
          nullable: false
          type: boolean
        error_msg:
          read_only: true
          description: ''
          nullable: false
          type: string
        creator: *id010
        created_at:
          read_only: true
          description: ''
          nullable: false
          type: string
          format: date-time
        has_error:
          readOnly: true
          description: ''
          type: boolean
        project: *id010
        rerun_all_submissions:
          read_only: true
          description: "When True, indicates that all submissions for the specified\n\
            \                     project should be rerun. Otherwise, only the submissions\n\
            \                     whose primary keys are listed in submission_pks\
            \ should be rerun."
          nullable: false
          type: boolean
        submission_pks:
          read_only: true
          description: "When rerun_all_submissions is False, specifies which submissions\n\
            \                     should be rerun."
          nullable: false
          type: array
          items:
            read_only: true
            description: ''
            nullable: false
            type: integer
        rerun_all_ag_test_suites:
          read_only: true
          description: "When True, indicates that all AGTestSuites belonging\n   \
            \                  to the specified project should be rerun. Otherwise,\n\
            \                     only the AGTestSuites specified in ag_test_suite_data\
            \ should\n                     be rerun."
          nullable: false
          type: boolean
        ag_test_suite_data:
          read_only: true
          description: "When rerun_all_ag_test_suites is False, specifies which\n\
            \                     AGTestSuites should be rerun and which AGTestCases\
            \ within\n                     those suites should be rerun.\n       \
            \ Data format:\n        {\n            // Note: JSON format requires that\
            \ keys are strings. Postgres\n            // doesn't seem to care, but\
            \ some JSON serializers might.\n            \"<ag_test_suite_pk>\": [<ag_test_case_pk>,\
            \ ...],\n            ...\n        }\n        If an ag_test_suite_pk is\
            \ mapped to an empty list, then all ag test cases\n        belonging to\
            \ that ag test suite will be rerun."
          nullable: false
          type: object
        rerun_all_student_test_suites:
          read_only: true
          description: "When True, indicates that all StudentTestSuites belonging\n\
            \                     to the specified project should be rerun. Otherwise,\n\
            \                     only the StudentTestSuites specified in student_test_suite_pks\n\
            \                     should be rerun."
          nullable: false
          type: boolean
        student_suite_pks:
          read_only: true
          description: "When rerun_all_student_test_suites is False, specifies which\n\
            \                     student test suites should be rerun."
          nullable: false
          type: array
          items:
            read_only: true
            description: ''
            nullable: false
            type: integer
    HandgradingRubric:
      type: object
      properties:
        pk: *id010
        project: *id010
        last_modified:
          read_only: true
          description: ''
          nullable: false
          type: string
          format: date-time
        points_style:
          read_only: false
          description: "Determines how total_points and total_possible_points are\
            \ calculated\n                     for HandgradingResults."
          nullable: false
          oneOf:
          - $ref: '#/components/schemas/PointsStyle'
        max_points:
          read_only: false
          description: "The denominator of a handgrading score.\n                \
            \     When points_style is \"start_at_zero_and_add\", this value\n   \
            \                  overrides the sum of positive Criteria point values\
            \ as the\n                     total points possible.\n              \
            \       When points_style is \"start_at_max_and_subtract\", this field\n\
            \                     is REQUIRED."
          nullable: true
          type: number
        show_grades_and_rubric_to_students:
          read_only: false
          description: "Whether students can see their handgrading scores,\n     \
            \                including information from the rubric."
          nullable: false
          type: boolean
        handgraders_can_leave_comments:
          read_only: false
          description: Whether handgraders can add comments to a HandgradingResult.
          nullable: false
          type: boolean
        handgraders_can_adjust_points:
          read_only: false
          description: Whether handgraders can edit HandgradingResult.point_adjustment.
          nullable: false
          type: boolean
        criteria:
          read_only: true
          description: ''
          nullable: false
          type: array
          items:
            $ref: '#/components/schemas/Criterion'
        annotations:
          read_only: true
          description: ''
          nullable: false
          type: array
          items:
            $ref: '#/components/schemas/Annotation'
    PointsStyle:
      type: string
      enum:
      - start_at_zero_and_add
      - start_at_max_and_subtract
    Criterion:
      type: object
      properties:
        pk: *id010
        handgrading_rubric: *id010
        last_modified:
          read_only: true
          description: ''
          nullable: false
          type: string
          format: date-time
        short_description:
          read_only: false
          description: A short description of this Criterion.
          nullable: false
          type: string
        long_description:
          read_only: false
          description: "A long description of this Criterion. Note that there is no\n\
            \                     enforced length difference between short_ and long_description.\n\
            \                     The separation is purely to be used by clients."
          nullable: false
          type: string
        points:
          read_only: false
          description: "The amount of points to add or subtract from a handgrading\
            \ score\n                     when selected."
          nullable: false
          type: number
    Annotation:
      type: object
      properties:
        pk: *id010
        handgrading_rubric: *id010
        short_description:
          read_only: false
          description: A short description of this Annotation.
          nullable: false
          type: string
        long_description:
          read_only: false
          description: "A long description of this Criterion. Note that there is no\n\
            \                     enforced length difference between short_ and long_description.\n\
            \                     The separation is purely to be used by clients."
          nullable: false
          type: string
        deduction:
          read_only: false
          description: "The amount of points to deduct from a handgrading score when\n\
            \                     applied. Must be non-positive."
          nullable: false
          type: number
        max_deduction:
          read_only: false
          description: "The maximum amount of points that can be cumulatively\n  \
            \                   deducted from a handgrading score by applications\
            \ of\n                     this annotation. Must be None or non-positive."
          nullable: true
          type: number
        last_modified:
          read_only: true
          description: ''
          nullable: false
          type: string
          format: date-time
    HandgradingResult:
      type: object
      properties:
        pk: *id010
        last_modified:
          read_only: true
          description: ''
          nullable: false
          type: string
          format: date-time
        submission: *id010
        handgrading_rubric:
          read_only: true
          description: The HandgradingRubric that this HandgradingResult is based
            on.
          nullable: false
          oneOf:
          - $ref: '#/components/schemas/HandgradingRubric'
        group: *id010
        applied_annotations:
          read_only: true
          description: ''
          nullable: false
          type: array
          items:
            $ref: '#/components/schemas/AppliedAnnotation'
        comments:
          read_only: true
          description: ''
          nullable: false
          type: array
          items:
            $ref: '#/components/schemas/Comment'
        criterion_results:
          read_only: true
          description: ''
          nullable: false
          type: array
          items:
            $ref: '#/components/schemas/CriterionResult'
        finished_grading:
          read_only: false
          description: "Handgraders should set this field to True when they are finished\n\
            \                     grading this group's submission."
          nullable: false
          type: boolean
        points_adjustment:
          read_only: false
          description: "An arbitrary adjustment to this result's total points.\n \
            \                    Note that this does not affect total points possible."
          nullable: false
          type: number
        submitted_filenames:
          readOnly: true
          description: "Returns a list of strings containing the filenames of the\
            \ Submission this result\n        belongs to."
          type: array
          items: *id011
        total_points:
          readOnly: true
          description: "The total number of points awarded. Note that it is possible\n\
            \        for this value to be greater than total_points."
          type: number
        total_points_possible:
          readOnly: true
          description: "The denominator of the handgrading score based on the\n  \
            \      handgrading rubric's points style and max points."
          type: number
    CriterionResult:
      type: object
      properties:
        pk: *id010
        last_modified:
          read_only: true
          description: ''
          nullable: false
          type: string
          format: date-time
        selected:
          read_only: false
          description: "When True, indicates that the criterion's point allotment\
            \ should be\n                     added to (or subtracted from if negative)\
            \ the total handgrading points."
          nullable: false
          type: boolean
        criterion:
          read_only: true
          description: The Criterion that the CriterionResult is tied to.
          nullable: false
          oneOf:
          - $ref: '#/components/schemas/Criterion'
        handgrading_result: *id010
    AppliedAnnotation:
      type: object
      properties:
        pk: *id010
        last_modified:
          read_only: true
          description: ''
          nullable: false
          type: string
          format: date-time
        location:
