          description: The source code location where the Annotation was applied.
          nullable: false
          oneOf:
          - $ref: '#/components/schemas/Location'
        annotation:
          read_only: true
          description: The Annotation that was applied to the source code.
          nullable: false
          oneOf:
          - $ref: '#/components/schemas/Annotation'
        handgrading_result: *id010
    Comment:
